        "@sveltejs/vite-plugin-svelte": "^3.1.2",
        "@tailwindcss/forms": "^0.5.9",
        "@tailwindcss/typography": "^0.5.14",
        "@types/crypto-js": "^4.2.2",
        "@types/node-windows": "^0.1.6",
        "@types/throttle-debounce": "^5.0.2",
